<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.measuringup"
    android:versionCode="1"
    android:versionName="1.0" xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/measure_up_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:logo="@drawable/measure_up_logo">
        <activity
            android:name=".MeasureUpActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.measuringup.EditPersonActivity"
            android:label="@string/app_name"
             />
        <activity
            android:name="com.example.measuringup.AddPersonActivity"
            android:label="@string/app_name"
             />
        <activity android:name="EditMeasurementActivity"></activity>
    </application>

</manifest>
